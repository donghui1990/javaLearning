package 匿名内部类的使用;

//匿名内部类的 方法的调用以及参数的传递

public class TestClass {
	private String name;
	private int age;

	public void input() {
		
		new A(){

			@Override
			public void test(String name, int age) {
				// TODO Auto-generated method stub
				System.out.println("姓名:"+name+" "+"年龄："+age);
				
			}

			
		}.test(name, age);
		
	}
	
	public void output() {
		System.out.println("姓名:"+name+" "+"年龄："+age);
		
		
	}
	
	public void setName(String name) {
		this.name=name;
	}
	public  void setAge(int age) {
		 this.age=age;
	 }
	 
	public  String retuenName() {
		return this.name;
		 
	 }
	
	public  int  retuenAge() {
		return this.age;
		 
	 }


}
